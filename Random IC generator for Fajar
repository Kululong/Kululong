    def Students(self):
        for num in range(100):
            First_letter = Letters
            Last_letter = Letters
            second_number = Numbers
            third_number = Numbers
            Fourth_number = Numbers
            Fifth_number = Numbers
            Sixth_number = Numbers
            Seventh_number = Numbers
            Eighth_number = Numbers
            First_letter = random.choice(Letters)
            Last_letter = random.choice(Letters)
            second_number = random.choice(Numbers)
            third_number = random.choice(Numbers)
            Fourth_number = random.choice(Numbers)
            Fifth_number = random.choice(Numbers)
            Sixth_number = random.choice(Numbers)
            Seventh_number = random.choice(Numbers)
            Eighth_number = random.choice(Numbers)
            Random_letters = random.sample(Letters, 9)
            Random_numbers = random.sample(Numbers, 9)
            Total = Random_numbers + Random_letters
            Total = random.sample(Total, 9)
            IC = f'{First_letter} {second_number} {third_number} {Fourth_number} {Fifth_number} {Sixth_number} {Seventh_number} {Eighth_number} {Last_letter}'
